	<queryString>
		<![CDATA[SELECT p.idPortafolio, ca.codDocente, d.nombreDocente ,s.nombreSemestre, ca.idCurso, c.nombre, p.fechaPortafolio, p.estadoPortafolio, p.recepcioadoPor, s.nombreSemestre, ca.alumnosMatriculados, ca.alumnosRetirados, ca.alumnosAbandono,p.estudianteAsisten,p.estudianteAprobado, p.estudianteDesaprobado
		FROM portafolio p
		LEFT JOIN cargaacademica ca
		ON p.idCargaAcademica= ca.idCargaAcademica
		LEFT JOIN semestre s
		ON ca.idSemestre = s.idSemestre
		LEFT JOIN docente d
		ON d.codDocente = ca.codDocente
		LEFT JOIN curso c
		ON ca.idCurso = c.idCurso
    WHERE p.idPortafolio=$P{ID};]]>
	</queryString>
